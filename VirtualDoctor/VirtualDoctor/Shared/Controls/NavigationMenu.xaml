<UserControl x:Class="VirtualDoctor.Shared.Controls.NavigationMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VirtualDoctor.Shared.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="800">
    <Viewbox>
        <ToolBarPanel>
            <ToolBar DockPanel.Dock="Top">
                <local:ThemeSelectorControl x:Name="ThemeSelector"></local:ThemeSelectorControl>
                <local:TranslateControl x:Name="Translate"></local:TranslateControl>
                <local:GoBackControl x:Name="GoBack"></local:GoBackControl>
            </ToolBar>
        </ToolBarPanel>
    </Viewbox>
</UserControl>
